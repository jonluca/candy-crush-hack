package logic;

import java.io.File;
import java.nio.file.Files;
import java.nio.file.Path;
import java.nio.file.Paths;

public class hexLogic {

    public hexLogic(File file) {
	Path source = new Paths.get(file.getAbsolutePath());
	Files.copy(file.getAbsolutePath(), file.getAbsolutePath() + ".bak", false);
    }
}
